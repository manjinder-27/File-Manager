<ImageViewer>:
	name:'imageview'
	MDFloatLayout:
		md_bg_color:0,0,0,1
		MDTopAppBar:
			id:appbar
			title:""
			left_action_items:[['arrow-left', lambda x:root.goBack()]]
			right_action_items:[['blank']]
			pos_hint:{'top':1}
			md_bg_color:0,0,0,1
			specific_text_color:1,1,1,1
		FitImage:
			id:img
			source:""
			size_hint:.95,.6
			pos_hint:{'center_x':.5,'center_y':.5}